    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.docentes && $data.titulacion && $data.titulacion_estudiantes && $data.titulacion_docente_emite_informe && $data.titulacion_director && $data.titulacion_codirector && $data.titulacion_tribunal_presidente && $data.titulacion_tribunal_miembro && $data.titulacion_sustentacion && $data.titulacion_director_sugerido && $data.titulacion_profesorFiscal ){",
        "\t//attach lazy references variables to parent variables",
        "\t$data.titulacion.estudiantes = $data.titulacion_estudiantes;",
        "\t$data.titulacion.docente_emite_informe = $data.titulacion_docente_emite_informe;",
        "\t$data.titulacion.director = $data.titulacion_director;",
        "\t$data.titulacion.codirector = $data.titulacion_codirector;",
        "\t$data.titulacion.tribunal_presidente = $data.titulacion_tribunal_presidente;",
        "\t$data.titulacion.tribunal_miembro = $data.titulacion_tribunal_miembro;",
        "\t$data.titulacion.sustentacion = $data.titulacion_sustentacion;",
        "\t$data.titulacion.director_sugerido = $data.titulacion_director_sugerido;",
        "\t$data.titulacion.profesorFiscal = $data.titulacion_profesorFiscal;",
        "\treturn {",
        "\t\t//map docentes variable to expected task contract input",
        "\t\tdocentesInput: {",
        "\t\t\tnombre: $data.docentes.nombre !== undefined ? $data.docentes.nombre : null,",
        "\t\t\tcorreo: $data.docentes.correo !== undefined ? $data.docentes.correo : null,",
        "\t\t\tci: $data.docentes.ci !== undefined ? $data.docentes.ci : null",
        "\t\t},",
        "\t\t//map titulacion variable to expected task contract input",
        "\t\ttitulacionInput: {",
        "\t\t\tagregar_mas_docentes: $data.titulacion.agregar_mas_docentes !== undefined ? $data.titulacion.agregar_mas_docentes : null",
        "\t\t}",
        "\t}",
        "}"
