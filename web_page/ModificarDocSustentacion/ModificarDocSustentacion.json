    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.context && $data.titulacion ){",
        "\treturn {",
        "\t\tdoc_solicitud_decano_sustentacion_aptitudDocInput: {",
        "\t\t\tid: $data.context.doc_solicitud_decano_sustentacion_aptitud_ref && $data.context.doc_solicitud_decano_sustentacion_aptitud_ref.id ? $data.context.doc_solicitud_decano_sustentacion_aptitud_ref.id.toString() : null,",
