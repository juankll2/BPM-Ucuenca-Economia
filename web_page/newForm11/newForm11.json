    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.personas && $data.titulacion && $data.titulacion_estudiantes && $data.titulacion_docente_emite_informe && $data.titulacion_director && $data.titulacion_codirector && $data.titulacion_tribunal_presidente && $data.titulacion_tribunal_miembro && $data.titulacion_sustentacion && $data.titulacion_director_sugerido && $data.titulacion_profesorFiscal ){",
        "\t//attach lazy references variables to parent variables",
        "\t$data.titulacion.estudiantes = $data.titulacion_estudiantes;",
        "\t$data.titulacion.docente_emite_informe = $data.titulacion_docente_emite_informe;",
        "\t$data.titulacion.director = $data.titulacion_director;",
        "\t$data.titulacion.codirector = $data.titulacion_codirector;",
        "\t$data.titulacion.tribunal_presidente = $data.titulacion_tribunal_presidente;",
        "\t$data.titulacion.tribunal_miembro = $data.titulacion_tribunal_miembro;",
        "\t$data.titulacion.sustentacion = $data.titulacion_sustentacion;",
        "\t$data.titulacion.director_sugerido = $data.titulacion_director_sugerido;",
        "\t$data.titulacion.profesorFiscal = $data.titulacion_profesorFiscal;",
        "\treturn {",
        "\t\t//map personas variable to expected task contract input",
        "\t\tpersonasInput: {",
        "\t\t\tnombre: $data.personas.nombre !== undefined ? $data.personas.nombre : null,",
        "\t\t\tcorreo: $data.personas.correo !== undefined ? $data.personas.correo : null,",
        "\t\t\tci: $data.personas.ci !== undefined ? $data.personas.ci : null,",
        "\t\t\tcorreo_personal: $data.personas.correo_personal !== undefined ? $data.personas.correo_personal : null,",
        "\t\t\tegresado: $data.personas.egresado !== undefined ? $data.personas.egresado : null",
